{
  "name": "nu-state",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/melnikov-s/nu-state.git"
  },
  "author": "Sergey Melnikov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/melnikov-s/nu-state/issues"
  },
  "homepage": "https://github.com/melnikov-s/nu-state#readme",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "module": "./dist/nu-state.es.js",
  "main": "./dist/nu-state.umd.js",
  "exports": {
    ".": {
      "import": "./dist/nu-state.es.js",
      "require": "./dist/nu-state.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "yarn build",
    "perf": "vitest -c vite.perf.config.js"
  },
  "dependencies": {
    "nu-observables": "workspace:*",
    "nu-reactive-graph": "workspace:*"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mobx": "^6.6.2",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.4.1",
    "vitest": "^0.23.2"
  }
}
